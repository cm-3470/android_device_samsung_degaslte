# mount point	fstype		device					device2
/efs			ext4		/dev/block/mmcblk0p3    flags=display="EFS";backup=1
/boot			emmc		/dev/block/mmcblk0p9
/recovery		emmc		/dev/block/mmcblk0p10
/cache			ext4		/dev/block/mmcblk0p19
/system			ext4		/dev/block/mmcblk0p18
/data			ext4		/dev/block/mmcblk0p21	length=-16384
/modem			emmc		/dev/block/mmcblk0p13
/preload		ext4		/dev/block/mmcblk0p20

/external_sd	auto		/dev/block/mmcblk1p1	flags=display="Micro SDcard";storage;wipeingui;removable
